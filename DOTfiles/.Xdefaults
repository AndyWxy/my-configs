Xft.dpi: 96
Xcursor.theme: Polar

## urxvt -----------------------------------------------------
urxvt.perl-ext-common: default,matcher
urxvt.urlLauncher: firefox
urxvt.matcher.button: 1
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

urxvt*depth: 32
urxvt.buffered: true
#urxvt.geometry: 110x40
#urxvt*background: rgba:0000/0000/0000/dddd
#urxvt.foreground: rgba:ffff/ffff/ffff/aaaa

#Input Method set
 urxvt*inputMethod: fcitx
 urxvt*preeditType: OverTheSpot
 urxvt*imFont: -wenquanyi-wenquanyi bitmap song-bold-r-normal--0-0-75-75-p-0-iso10646-1
#urxvt*imFont: -microsoft-microsoft yahei-medium-r-normal--0-0-0-0-p-0-iso10646-1

#urxvt*termName: playincli
urxvt*cutchars: `"()'*<>[]{|}

## borderless and no scrollbar
urxvt*scrollBar_right: false
urxvt*scrollBar: false
urxvt*borderLess: true
urxvt*cursorColor:        #729FCF
urxvt*cursorColor2:        #000000
urxvt*cursorBlink:        false


## Transparency setting
urxvt*transparent: true
#urxvt*fading: 80
urxvt*blurRadius: 20x30
#urxvt*inheritPixmap: true
#urxvt*tintColor: black
urxvt*shading: 17

## geometry and font
#urxvt*geometry: 84Ã—24
#urxvt*font: xft:terminus:pixelsize=12
#urxvt*font: xft:smoothansi:pixelsize=7
urxvt.font: xft:smoothansi-7,xft:WenQuanYi Bitmap Song-7
#:WenQuanYi Bitmap Song:pixelsize=7
#urxvt.font: xft:Luxi Mono:size=11,xft:Microsoft YaHei:size=13

